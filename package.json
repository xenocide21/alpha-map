{
  "name": "oberin-map",
  "version": "0.2.0",
  "description": "Interactive World Map of Oberin",
  "main": "obfuscatedMain.js",
  "scripts": {
    "start": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
